pipeline {
  // agent {
  //   node {
  //     label 'maven'
  //   }
  }
  
  
  stages{
      stage('checkout'){
          steps {
            container('maven'){
              checkout(scm)
            }
            
          }
        
      }
    
    
    stage('fosscheck'){
          steps {
              container('maven-fosscheck'){
                    sh 'ls -l'
                    sh 'pwd'
                    sh 'java -jar cli-extractor-1.1-RELEASE.jar --help'
                  // fosscheck endpoint: 'https://trial.fosscheck.com', outputDir: '/home/jenkins/agent/workspace/test6pv4m_fosscheck_fosscheck', 
                  // projectToken: '', 
                  // token: ''
                  
              }
            }
        }
    
  }
  




}